/*
 * 006spi_tx_testing.c
 *
 */

#include "stm32407xx.h"
/*
 *  PB12	--> 	SPI2_NSS
 *  PB13	--> 	SPI2_SCLK
 *  PB14	--> 	SPI2_MISO
 *  PB15	--> 	SPI2_MOSI
 *  ALT function mode 	: 	5
 */

void SPI_GPIOInits(void)
{
	GPIO_Handle_t SPIPins;

	SPIPins.pGPIOx = GPIOB;
	SPIPins.GPIO_PinConfig.GPIO_PinMode = GPIO_MODE;
}


int main (void)
{
	// initialize the GPIO pins to behave SPI2 pins
	SPI_GPIOInits();


}
